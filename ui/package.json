    "@vue/test-utils": "1.0.3",
    "autoprefixer": "^9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "beautifier": "^0.1.7",
    "chalk": "4.1.0",
    "chokidar": "3.4.2",
