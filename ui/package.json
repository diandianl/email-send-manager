    "email",
    "element-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diandianl/email-send-manager.git"
  },
  "bugs": {
    "url": "https://github.com/diandianl/email-send-manager/issues"
  },
  "dependencies": {
